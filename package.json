{
  "name": "@urbanshona/browsifier",
  "version": "1.0.0",
  "description": "A Command Line App to remove decorators from TypeORM Entity Definitions",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack --mode=production",
    "dev": "ts-node  -r tsconfig-paths/register ./src/index.ts -s server/entities -o browser/entities"
  },
  "author": {
    "name": "Desmond Rgwaringesu",
    "email": "desmondrg@gmail.com"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.12.16",
    "@babel/plugin-proposal-class-properties": "~7.12.13",
    "@babel/plugin-proposal-decorators": "~7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "~7.12.13",
    "@babel/preset-env": "~7.12.16",
    "@babel/preset-typescript": "~7.12.16",
    "babel-loader": "~8.2.2",
    "babel-plugin-module-resolver": "~4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "~6.1.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "~4.1.5",
    "webpack": "~5.22.0",
    "webpack-cli": "~4.5.0",
    "yargs": "~16.2.0"
  },
  "dependencies": {
    "chalk": "*"
  }
}
